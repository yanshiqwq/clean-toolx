{
	"name": "系统清理",
	"author": "延时qwq",
	"version_name": "v1.2",
	"version_code": 3,
	"platform": "windows",
	"rules": [
		{
			"name": "Windows目录临时文件",
			"warning_level": 0,
			"paths": [
				{
					"scan_path": "${WINDIR}\\Temp\\",
					"path": "${WINDIR}\\Temp\\.+"
				}
			]
		},{
			"name": "AppData目录临时文件",
			"warning_level": 0,
			"paths": [
				{
					"scan_path": "${TEMP}\\",
					"path": "${TEMP}\\.+"
				},{
					"scan_path": "${USERPROFILE}\\AppData\\LocalLow\\Temp\\",
					"path": "${USERPROFILE}\\AppData\\LocalLow\\Temp\\.+"
				}
			]
		}
	]
}